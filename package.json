{
  "name": "xpdb-monorepo",
  "version": "0.0.0",
  "private": true,
  "description": "Monorepo for xpdb packages",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "npm run build --workspaces",
    "build:schema": "npm run build --workspace=packages/xpdb-schema",
    "build:viewer": "npm run build --workspace=packages/xpdb-viewer",
    "clean": "npm run clean --workspaces && rm -rf node_modules"
  },
  "keywords": [
    "database",
    "schema",
    "orm",
    "monorepo"
  ],
  "author": "",
  "license": "Unlicense",
  "repository": {
    "type": "git",
    "url": ""
  },
  "devDependencies": {
    "@types/node": "^20.19.25",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
